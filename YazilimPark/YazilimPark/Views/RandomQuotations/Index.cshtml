<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Rastgele Özlü Söz Uygulaması</title>
</head>

<div class="homepage">
    <div class="row">
        <div class="col-12">
            <img src="data:image/png;base64," style="border: 5px solid #000000; border-radius: 30px;" alt="Alternate Text" />
            <br>
            <br>
        </div>
    </div>
</div>


    <body>

        <div>
            <h2>Özlü Söz</h2>
            <ul id="quotations" />
        </div>
        <div>
            <h2>Search by ID</h2>
            <input type="text" id="prodId" size="5" />
            <input type="button" value="Search" onclick="find();" />
            <p id="quotation" />
        </div>

        <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.3.min.js"></script>
        <script>
            var uri = 'api/quotations';

            $(document).ready(function () {
                // Send an AJAX request
                $.getJSON(uri)
                    .done(function (data) {
                        // On success, 'data' contains a list of products.
                        $.each(data, function (key, item) {
                            // Add a list item for the product.
                            $('<li>', { text: formatItem(item) }).appendTo($('#quotations'));
                        });
                    });
            });

            function formatItem(item) {
                return item.Quotation + ': $' + item.Quotation;
            }

            function find() {
                var id = $('#prodId').val();
                $.getJSON(uri + '/' + id)
                    .done(function (data) {
                        $('#quotation').text(formatItem(data));
                    })
                    .fail(function (jqXHR, textStatus, err) {
                        $('#quotation').text('Error: ' + err);
                    });
            }
        </script>
    </body>
</html>